
<h1>Deliveries:</h1>
<table id="maintable">
     <tr>
        <th><b>Date:</b></th>
        <th><b>Task:</b></th>
        <th><b>Recepient:</b></th>
        <th><b>Volunteer:</b></th>
    </tr>
    <% @deliveries.each do |delivery| %>
    <tr>
        <td><%= delivery.date %></td>
        <td><a href="/deliveries/<%= delivery.id %>"><%= delivery.task %></a></td>
        <td><%= delivery.recepient.name %></td>
        <td><%= delivery.volunteer.name %></td>
    </tr>
    <% end %>

</table>
